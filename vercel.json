{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/php/(.*)",
        "dest": "/public/php/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo_uri",
      "SESSION_SECRET": "@session_secret",
      "REDIS_HOST": "@redis_host",
      "REDIS_PORT": "@redis_port",
      "NODE_ENV": "production"
    }
  }
  