{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/register",
      "dest": "/api/register"
    },
    {
      "src": "/login",
      "dest": "/api/login"
    },
    {
      "src": "/profile",
      "dest": "/profile.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
